<resources>
    <color name="primaryDark">#273238</color>
    <color name="window">#313335</color>
    <color name="textPrimaryColor">@color/white</color>
    <color name="primary">#4fc3f7</color>
    <color name="drawerHeaderBackground">#1b91cf </color>
    <color name="surface">#333a40</color>
    <color name="category">#36474f</color>

    <!-- Base application theme. -->
    <style name="Theme.GPro" parent="Theme.MaterialComponents.DayNight.NoActionBar">

        <item name="colorPrimary">@color/primary</item>
        <item name="colorOnPrimary">@color/onPrimary</item>
        <item name="colorPrimaryVariant">@color/purple_700</item>

        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/primary</item>
        <item name="colorSecondaryVariant">@color/teal_200</item>
        <item name="colorOnSecondary">@color/onPrimary</item>

        <!-- Customize your theme here. -->
        <item name="android:textColorPrimary">@color/textPrimaryColor</item>
        <item name="android:windowBackground">@color/window</item>
        <item name="colorSurface">@color/surface</item>
        <item name="colorOnSurface">@color/white</item>
        <item name="colorPrimaryDark">@color/primaryDark</item>
    </style>
</resources>